{
  "name": "dissonance/http-cookie",
  "description": "Slim (10Kb) implementation of working with cookies, with psr-15 middleware, without dependencies (psr interfaces only).",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT" ,
  "keywords": [
    "dissonance",
    "psr7 cookie",
    "psr15",
    "cookie",
    "cookie middleware"
  ],
  "require": {
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "dissonance/tests-http-cookie": "1.0.0"
  },
  "suggest": {
    "dissonance/web" : "Full build of the web version of the Dissonance framework with Http-Kernel, Middlewares etc....",
    "dissonance/http-middlewares-support" : "PSR middleware dispatcher",
    "nyholm/psr7": "Http Message PSR-7 realisation"
  },
  "autoload": {
    "psr-4": {
      "\\Dissonance\\Http\\Cookie": "src/"
    }
  },
  "extra": {
    "dissonance" : {
      "providers" : "Dissonance\\Http\\Cookie\\CookiesProvider"
    }
  }
}