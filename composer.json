{
  "name": "symbiotic/http-cookie",
  "description": "Slim (10Kb) implementation of working with cookies, with psr-15 middleware, without dependencies (psr interfaces only).",
  "version": "1.4.0",
  "type": "library",
  "license": "MIT" ,
  "keywords": [
    "symbiotic",
    "psr7 cookie",
    "psr15",
    "cookie",
    "cookie middleware"
  ],
  "require": {
    "php": ">=8.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "symbiotic/tests-http-cookie": "1.4.0"
  },
  "suggest": {
    "nyholm/psr7": "Http Message PSR-7 realisation"
  },
  "autoload": {
    "psr-4": {
      "Symbiotic\\Http\\Cookie\\": "src/Http/Cookie"
    }
  },
  "extra": {
    "Symbiotic" : {
      "providers" : "Symbiotic\\Http\\Cookie\\CookiesProvider"
    }
  }
}